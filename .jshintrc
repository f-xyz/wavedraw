{
    "node": true,
    "browser": true,
    "esnext": true,
    "camelcase": true,
    "curly": false,
    "eqeqeq": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "maxlen": 120,
    "newcap": true,
    "noarg": true,
    "quotmark": "single",
